package inheritanceExample;

import day4.staticdemo;

class A{
	
	int a =100;
	void display(){
		System.out.println(a);
		
	}
	
}

class B extends A
{
	int b =100;
	void display(){
		System.out.println(b);
		System.out.println(a);
	}
}

public class InheritExample {
	

	public static void main(String[] args) {
		
		B obj = new B();
		obj.display();
	}

}
